@model Slate.Web.Presentation.Home.IssueListModel

<div class="work">
	<h2>Work on an issue</h2>
</div>

<div class="open">
	<img class="add" />
	<h2>@Html.RenderLink(Model.OpenLink)</h2>
</div>

<div class="issues">
	@{Html.RenderList(Model.List);}
</div>

<div class="explore">
	@Html.RenderLink(Model.ExploreItem.Number.Link)
	<div class="subtitle">@Model.ExploreItem.Description</div>
</div>